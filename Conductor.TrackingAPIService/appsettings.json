{
    "Logging": {
        "LogLevel": {
            "Default": "Information",
            "Microsoft": "Warning",
            "Microsoft.Hosting.Lifetime": "Information"
        }
    },
    "AllowedHosts": "*",
    "ConsulSetting": {
        "ServiceName": "TrackingAPI",
        "ServiceIP": "localhost",
        "ServicePort": 10002,
        "ServiceHealthCheck": "/api/healthcheck",
        "ConsulAddress": "http://localhost:8500" //注意，docker容器内部无法使用localhost访问宿主机器，如果是控制台启动的话就用localhost
    }
}
